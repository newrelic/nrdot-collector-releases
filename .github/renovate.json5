{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "automerge": false,
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "(^|/)test-spec.yaml$"
      ],
      "matchStringsStrategy": "combination",
      "matchStrings": [
        "(?<registryUrl>newrelic)\\/(?<depName>nr-k8s-otel-collector)\\s+version:\\s+(?<currentValue>[\\d\\.]+)",
      ],
      "datasourceTemplate": "helm",
      "versioningTemplate": "semver"
    }
  ]
}